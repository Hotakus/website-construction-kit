					 "--enable-libgcc", "--enable-mysqlnd", "--enable-sockets", "--enable-soap",
					 "--enable-libgcc", "--with-openssl", "--with-curl", "--enable-gd", "--enable-intl",
					 "--enable-mbstring", "--enable-exif"],
	"":""
}
